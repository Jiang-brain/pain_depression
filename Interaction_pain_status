fit0<- coxph(Surv(time, status) ~ pain+age+gender+ethnicity+deprivation+income+university_education+smoking+alcohol+sedentary+vascular_history+cancer+obesity+diabetes,data = datas)
fit1<- coxph(Surv(time, status) ~ pain*age+gender+ethnicity+deprivation+income+university_education+smoking+alcohol+sedentary+vascular_history+cancer+obesity+diabetes,data = datas)
fit2<- coxph(Surv(time, status) ~ pain*gender+age+ethnicity+deprivation+income+university_education+smoking+alcohol+sedentary+vascular_history+cancer+obesity+diabetes,data = datas)
fit3<- coxph(Surv(time, status) ~ pain*ethnicity+age+gender+deprivation+income+university_education+smoking+alcohol+sedentary+vascular_history+cancer+obesity+diabetes,data = datas)
fit4<- coxph(Surv(time, status) ~ pain*deprivation+age+gender+ethnicity+income+university_education+smoking+alcohol+sedentary+vascular_history+cancer+obesity+diabetes,data = datas)
fit5<- coxph(Surv(time, status) ~ pain*income+age+gender+ethnicity+deprivation+university_education+smoking+alcohol+sedentary+vascular_history+cancer+obesity+diabetes,data = datas)
fit6<- coxph(Surv(time, status) ~ pain*university_education+age+gender+ethnicity+deprivation+income+smoking+alcohol+sedentary+vascular_history+cancer+obesity+diabetes,data = datas)
fit7<- coxph(Surv(time, status) ~ pain*smoking+age+gender+ethnicity+deprivation+income+university_education+alcohol+sedentary+vascular_history+cancer+obesity+diabetes,data = datas)
fit8<- coxph(Surv(time, status) ~ pain*alcohol+age+gender+ethnicity+deprivation+income+university_education+smoking+sedentary+vascular_history+cancer+obesity+diabetes,data = datas)
fit9<- coxph(Surv(time, status) ~ pain*sedentary+age+gender+ethnicity+deprivation+income+university_education+smoking+alcohol+vascular_history+cancer+obesity+diabetes,data = datas)
fit10<-coxph(Surv(time, status) ~ pain*vascular_history+age+gender+ethnicity+deprivation+income+university_education+smoking+alcohol+sedentary+cancer+obesity+diabetes,data = datas)
fit11<-coxph(Surv(time, status) ~ pain*cancer+age+gender+ethnicity+deprivation+income+university_education+smoking+alcohol+sedentary+vascular_history+obesity+diabetes,data = datas)
fit12<-coxph(Surv(time, status) ~ pain*obesity+age+gender+ethnicity+deprivation+income+university_education+smoking+alcohol+sedentary+vascular_history+cancer+diabetes,data = datas)
fit13<-coxph(Surv(time, status) ~ pain*diabetes+age+gender+ethnicity+deprivation+income+university_education+smoking+alcohol+sedentary+vascular_history+cancer+obesity,data = datas)
fit_models=list(fit1,fit2,fit3,fit4,fit5,fit6,fit7,fit8,fit9,fit10,fit11,fit12,fit13)
rm(fit1,fit2,fit3,fit4,fit5,fit6,fit7,fit8,fit9,fit10,fit11,fit12,fit13)
P_interaction=data.frame(matrix(ncol = 1, nrow = 13))
row.names(P_interaction)=c('Age','Sex','Race','Deprivation','Income','University_education','Smoking','Alcohol','Sedentary','Vascular','Cancer','Obesity','Diabetes')
colnames(P_interaction)=c('P_interaction')
for (i in c(1:13)) {
  P_interaction$P_interaction[i]=signif(anova(fit0,fit_models[[i]],test="Chisq")$`Pr(>|Chi|)`[2],3)
}
rm(i,fit0,fit_models)
